<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AC0001Validatelength_of_Industry</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(
				REGEX(
								TEXT(Industry),   
								&quot;[a-zA-Z-. ]{0,6}$&quot;
				)
)</errorConditionFormula>
    <errorMessage>No se puede crear una industria con mas de 6 caracteres</errorMessage>
</ValidationRule>
